#:kivy 1.10
FloatLaytout:
    Button:
        #Anchor point: Canto Superior Direito
        #Posiciona o widget a 300px da borda esquerda em relação ao widget
        right: 300
        
        #Anchor point: Limite superior do widget
        #Posiciona o widget a 20px da borda inferior
        top: 20

        #Anchor Point: Centro horizontal
        #Posiciona o widget a 5 px da borda em relação ao centro do widget
        center_x: 5

        #Anchor Point: Centro vertical
        center_y: 4 